Now(formId) {\r\n    return fetch(baseUrl + formId, { method: \"DELETE\", headers: authHeader() })\r\n        .then(handleResponse)\r\n        .catch(handleError);\r\n}","C:\\Users\\Slightom\\Desktop\\MyNewProjects\\reactGoldenHand\\react-golden-hand\\src\\api\\apiUtils.js",["265"],"import { logout } from \"../components/common/Helper\";\r\n\r\nconst baseUrl = process.env.REACT_APP_API_URL;\r\n\r\n\r\nexport async function handleResponse(response) {\r\n    debugger;\r\n    if (response.ok)\r\n        return response.json();\r\n    if (response.status === 400) {\r\n        // So, a server-side validation error occurred.\r\n        // Server side validation returns a string error message, so parse as text instead of json.\r\n        const error = await response.text();\r\n        throw new Error(error);\r\n    }\r\n    if (response.status === 401) {\r\n        logout();\r\n        debugger;\r\n        const error = await response.status;;\r\n        throw new Error(error);\r\n    }\r\n    else\r\n        throw new Error(\"Network response was not ok. Sorry\");\r\n}\r\n\r\n// In a real app, would likely call an error logging service.\r\nexport function handleError(error) {\r\n    debugger;\r\n    // eslint-disable-next-line no-console\r\n    console.error(\"API call failed. \" + error);\r\n    throw error;\r\n}","C:\\Users\\Slightom\\Desktop\\MyNewProjects\\reactGoldenHand\\react-golden-hand\\src\\components\\forms\\FormList.js",[],"C:\\Users\\Slightom\\Desktop\\MyNewProjects\\reactGoldenHand\\react-golden-hand\\src\\redux\\reducers\\seniorReducer.js",[],"C:\\Users\\Slightom\\Desktop\\MyNewProjects\\reactGoldenHand\\react-golden-hand\\src\\redux\\actions\\seniorActions.js",[],"C:\\Users\\Slightom\\Desktop\\MyNewProjects\\reactGoldenHand\\react-golden-hand\\src\\api\\seniorApi.js",[],"C:\\Users\\Slightom\\Desktop\\MyNewProjects\\reactGoldenHand\\react-golden-hand\\src\\redux\\reducers\\initialState.js",[],"C:\\Users\\Slightom\\Desktop\\MyNewProjects\\reactGoldenHand\\re